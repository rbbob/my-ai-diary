{
  "buildCommand": "cd client && npm ci && npx vite build",
  "outputDirectory": "client/dist",
  "installCommand": "cd client && npm ci",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    },
    "api/*/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}